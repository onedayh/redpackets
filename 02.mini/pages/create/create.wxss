@keyframes circleProgressLoad_left{
    0%, 50%{
        transform: rotate(45deg);
    }
    100%{
        transform: rotate(225deg);
    }
}
@keyframes circleProgressLoad_right{
    0%{
        transform: rotate(45deg);
    }
    50%, 100%{
        transform: rotate(225deg);
    }
}
@keyframes helper {
    0%, 75%, 100%{
        transform: rotate(0);
    }
    25%{
        transform: rotate(6deg);
    }
    75%{
        transform: rotate(-6deg);
    }
}

