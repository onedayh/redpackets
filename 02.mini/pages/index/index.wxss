/*模板*/
.tem{
    display: flex;flex-direction: column;align-items: center;padding-top: 310rpx;padding-left: 100rpx;padding-right: 100rpx;
}
.tem-logo{
    width: 120rpx;height: 120rpx;border-radius: 50%;margin-bottom: 50rpx;
}
.tem-topic{
    color: #fff;font-size: 36rpx;padding-bottom: 30rpx;
}
.tem-bless{
    color: #fff;font-size: 30rpx;line-height: 44rpx;
}
.tem-open{
    width: 208rpx;height: 208rpx;border-radius: 50%;position: absolute;left: 50%;top: 710rpx;transform: translateX(-104rpx);
}


.music{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    /*top: 130rpx;*/
    right: 36rpx;
    z-index: 14;
    transform-origin: 30rpx 30rpx;
    animation: musicRotate 2s linear infinite;
}
.music-pause{
    animation: none
}
.music-pause::after{
    position: absolute;
    content: '';
    width: 1px;
    height: 60rpx;
    background-color: #333;
    top: 0;
    left: 30rpx;
    transform: rotate(-30deg);
    transform-origin: 1px 30rpx;
}
.test-bg{
    width: 100%;
    height: 100%;
    background: linear-gradient(#ffdba3, #F47139);
    position: fixed;
    z-index: 0;
    left: 0;
