@keyframes beat{
    0% {
        transform: scale(1, 1)
    }
    10% {
        transform: scale(1.1, 1.1)
    }
    100% {
        transform: scale(1, 1)
    }
}
@keyframes redRotate{
    0% {transform: rotate3d(0, 1, 0, 0);opacity: 0 }
    25% {transform: rotate3d(0, 1, 0, 90deg);opacity: .25 }
    50% {transform: rotate3d(0, 1, 0, 180deg);opacity: .5 }
    75% {transform: rotate3d(0, 1, 0, 270deg) ;opacity: .75}
    100% {transform: rotate3d(0, 1, 0, 360deg);opacity: 1 }

}
page{
    height: 100%;
}
.sdsd{
    background-color: #ffdba3;
}

/*模板*/
.wrap{
    height: 100%;
    background-color: transparent;
    position: relative;
    z-index: 1;
}
.open-form, active-form{
    width: auto;
}
.open-btn{
    background-color: transparent;
    padding: 0;
