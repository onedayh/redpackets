{
  "apps": [
    {
      "name": "teafun.api",
      "cwd": "/srv/node-app/current",
      "script": "bin/www",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "/usr/local/application/log/teafun/api/node-app.stderr.log",
      "out_file": "/usr/local/application/log/teafun/api/node-app.stdout.log",
      "pid_file": "/usr/local/application/log/teafun/api/node-geo-api.pid",
      "instances": 1,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "200M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "fork",
      "autorestart": true,
      "vizion": false
    }
  ]
}
