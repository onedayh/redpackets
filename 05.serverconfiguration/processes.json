{
  "apps": [
    {
      "name": "shitenai",
      "cwd": "/usr/local/application/web/",
      "script": "shitenai/bin/www",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "/usr/local/application/log/shitenai/node-app.stderr.log",
      "out_file": "/usr/local/application/log/shitenai/node-app.stdout.log",
      "pid_file": "/usr/local/application/log/shitenai/node-geo-api.pid",
      "instances": 2,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "300M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster",
      "autorestart": true,
      "vizion": false
    },
